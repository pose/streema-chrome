{
  "name": "Radio Player for Streema",
  "version": "0.2.4",
  "background_page": "background.html",
  "description": "Welcome to the best way to experience Streema in Chrome. Install this radio tuner and bring radio back into your life!",
  "icons": {
    "16": "img/streema_logo_small.png",
    "48": "img/streema_logo_alt2.png",
    "128": "img/streema_logo_alt2.png"
  },
  "browser_action": {
    "default_icon": "img/streema_logo_alt2.png",
    "default_title": "Radio Player for Streema",
    "popup": "popup.html"
  },
  "permissions": [
    "http://streema.com/*",
    "notifications"
  ],
  "options_page": "options.html",
  "minimum_chrome_version": "5"
}
